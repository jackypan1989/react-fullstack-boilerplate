{
  "name": "react-fullstack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate with React, Graphql, Typescript, Codegen, MongoDB, NextJS, Parcel, Linaria",
  "main": "index.js",
  "repository": "https://github.com/jackypan1989/react-fullstack-boilerplate",
  "author": "jackypan1989 <jackypan1989@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -r \"yarn:type-check:watch\" \"yarn:start\"",
    "start": "parcel ./src/client/index.html",
    "build": "yarn run type-check && parcel build ./src/client/index.html --public-url '/'",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.6.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.0.4",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
